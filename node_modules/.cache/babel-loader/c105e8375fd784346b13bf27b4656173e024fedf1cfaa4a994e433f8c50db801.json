div>\n    </div>\n  </div>\n</template>\n<script>\nimport { emails } from \"../db/emails\";\nimport showEmail from \"./showEmail.vue\";\nexport default {\n  components: { showEmail },\n  data() {\n    return {\n      emails: [],\n      openEmail: false,\n      emailData: null,\n      selected:[1,2,5]\n    };\n  },\n  created() {\n    this.emails = emails.emails;\n  },\n  methods: {\n    subject(messag) {\n      let messages = messag.replace(/<\\/?[^>]+(>|$)/g, \"\");\n      return messages.substr(0, 100) + \"....\";\n    },\n    showEmaildata(data) {\n      this.openEmail = true;\n      this.emailData = data;\n      this.emails.map(i => {\n        if( i.id == data.id){\n          return i.isRead = true\n        }\n       \n      }\n);\n     \n    },\n    close() {\n      this.openEmail = false;\n    },\n    search(event) {\n      let text = event.target.value;\n      let newArr = this.emails;\n      if (text.length >= 3) {\n        newArr = emails.emails.filter((i) => {\n          let ssss = text.toLocaleLowerCase()\n\n          return i.subject\n            .toLocaleLowerCase()\n            .startsWith(ssss) ||\n            i.from.name\n            .toLocaleLowerCase()\n            .startsWith(ssss)\n          });\n      }\n\n      if (text == \"\") {\n        newArr = emails.emails;\n      }\n\n      this.emails = newArr;\n    },\n    select(id){\n      this.selected.push(id)\n      \n      \n      \n    }\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n@import \"../assets/style_scss/components/emailStyle.scss\";\n</style>"],"mappings":";AAwHA,SAASA,MAAK,QAAS,cAAc;AACrC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,eAAe;EACbC,UAAU,EAAE;IAAED;EAAU,CAAC;EACzBE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLH,MAAM,EAAE,EAAE;MACVI,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAC,CAAC,CAAC,EAAC,CAAC,EAAC,CAAC;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,MAAK,GAAIA,MAAM,CAACA,MAAM;EAC7B,CAAC;EACDQ,OAAO,EAAE;IACPC,OAAOA,CAACC,MAAM,EAAE;MACd,IAAIC,QAAO,GAAID,MAAM,CAACE,OAAO,CAAC,iBAA