 \"\";\n    },\n    translat() {\n      const projectId = \"spring-nova-400108\";\n      const translate = new Translate({\n        projectId: projectId,\n      });\n      const text = 'Hello, world!';\n      const target = 'ru';\n      translate\n  .translate(text, target)\n  .then(results => {\n    const translation = results[0];\n\n    console.log(`Text: ${text}`);\n    console.log(`Translation: ${translation}`);\n  })\n  .catch(err => {\n    console.error('ERROR:', err);\n  });\n\n    },\n  },\n  computed: {},\n};\n</script>\n<style lang=\"scss\" scoped>\n@import \"../assets/style_scss/components/chat.scss\";\n</style>"],"mappings":";AAmCA,MAAM;EAAEA;AAAU,IAAIC,OAAO,CAAC,yBAAyB,CAAC;AACxD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE,CACR;QACEC,IAAI,EAAE,KAAK;QACXH,OAAO,EAAE,qDAAqD;QAC9DI,IAAI,EAAE,IAAIC,IAAI,EAAE,CAACC,QAAQ,EAAE;QAC3BC,IAAI,EAAE;MACR,CAAC,EACD;QACEJ,IAAI,EAAE,MAAM;QACZH,OAAO,EAAE,4DAA4D;QACrEI,IAAI,EAAE,IAAIC,IAAI,EAAE,CAACC,QAAQ,EAAE;QAC3BC,IAAI,EAAE;MACR,CAAC,EACD;QACEJ,IAAI,EAAE,MAAM;QACZH,OAAO,EAAE,SAAS;QAClBI,IAAI,EAAE,IAAIC,IAAI,EAAE,CAACC,QAAQ,EAAE;QAC3BC,IAAI,EAAE;MACR,CAAC,CACF;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACV,OAAM,IAAK,EAAE,EAAE;QACtB,IAAI,CAACE,QAAQ,CAACS,IAAI,CAAC;UACjBC,IAAI,EAAE,MAAM;UACZZ,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBI,IAAI,EAAE,IAAIC,IAAI,EAAE,CAACQ,QAAQ,EAAE;UAC3BN,IAAI,EAAE,IAAI,CAACC;QACb,CAAC,CAAC;MACJ;MACA,IAAI,CAACR,OAAM,GAAI,EAAE;IACnB,CAAC;IACDc,QAAQA,CAAA,EAAG;MACT,MAAMC,SAAQ,GAAI,oBAAoB;MACtC,MAAMC,SAAQ,GAAI,IAAInB,SAAS,CAAC;QAC9BkB,SAAS,EAAEA;MACb,CAAC,CAAC;MACF,MAAME,IAAG,GAAI,eAAe;MAC5B,MAAMC,MAAK,GAAI,IAAI;MACnBF,SAAQ,CACXA,SAAS,CAACC,IAAI,EAAEC,MAAM,EACtBC,IAAI,CAACC,OAAM,IAAK;QACf,MAAMC,WAAU,GAAID,OAAO,CAAC,CAAC,CAAC;QAE9BE,OAAO,CAACC,GAAG,CAAE,SAAQN,IAAK,EAAC,CAAC;QAC5BK,OAAO,CAACC,GAAG,CAAE,gBAAeF,WAAY,EAAC,CAAC;MAC5C,CAAC,EACAG,KAAK,CAACC,GAAE,IAAK;QACZH,OAAO,CAACI,KAAK,CAAC,QAAQ,EAAED,GAAG,CAAC;MA