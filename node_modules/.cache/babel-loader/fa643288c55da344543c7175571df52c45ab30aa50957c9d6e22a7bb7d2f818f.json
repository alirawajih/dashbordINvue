lor: #283046;\n  transition: width 0.4s;\n  /* overflow-x: hidden; Hide horizontal scrollbar */\n  /* overflow-y: scroll;  */\n  z-index: 3;\n}\n\n.menuOpen {\n  position: fixed;\n  width: 225px;\n  height: 100%;\n  display: block;\n  border-radius: 0px;\n  background-color: #283046;\n  transition: width 0.1s;\n  z-index: 3;\n}\n\n.text-none {\n  display: none;\n}\n\n.comint {\n  color: rgba(255, 255, 255, 0.33);\n}\n\n.comint:hover {\n  cursor: default;\n}\n\nul {\n  color: white;\n  list-style-type: none;\n  padding: 0px;\n}\n\nli {\n  /*margin: 5px;*/\n  width: 100%;\n  padding: 3px;\n  display: flex;\n  gap: 6px;\n  font-size: 15px;\n  cursor: pointer;\n}\n\n.link:hover {\n  margin-left: 8px;\n  color: white;\n}\n\n.logo {\n  width: 50px;\n}\n\na {\n  text-decoration: none;\n  color: white;\n}\n\nspan {\n  padding-left: 10px;\n  justify-content: right;\n  font-size: 19px;\n  display: flex;\n}\n\n.chevron {\n  position: absolute;\n  right: 25px;\n}\n\n.icon {\n  color: #7367f0;\n  cursor: pointer;\n  position: absolute;\n  right: 25px;\n  top: 8px;\n}\n\n.logotop {\n  background: #283046;\n}\n\n@media screen and (max-width: 992px) {\n  /* .menuOpen, .menu {\n    width: 37%;\n    height: 100%;\n    display: block;\n    border-radius: 0px;\n    background-color: rgb(30, 30, 91);\n  } */\n}\n</style>\n"],"mappings":"AAkFA,OAAOA,IAAG,MAAO,0BAA0B;AAE3C,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLH,IAAI,EAAEA,IAAI;MACVI,SAAS,EAAE,UAAU;MACrBC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE,EAAE;MACRC,IAAI,EAAE,cAAc;MACpBC,QAAQ,EAAE,MAAM;MAChBC,OAAO,EAAE,EAAE;MACXC,IAAI,EAAE,KAAK;MACXC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,EAAEA,CAAA,EAAG;MACH,IAAI,IAAI,CAACL,QAAO,IAAK,MAAM,EAAE;QAC3B,IAAI,CAACA,QAAO,GAAI,QAAQ;QACxB,IAAI,CAACM,QAAQ,EAAE;QACf,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,YAAY,CAAC;QAChC,IAAI,CAACD,MAAM,CAACC,MAAM,CAAC,eAAe,CAAC;QACnC;MACF,OAAO;QACL,IAAI,CAACR,QAAO,GAAI,MAAM;QACtB,IAAI,CAACS,QAAQ,EAAE;QACf,IAAI,CAACF,MAAM,CAACC,M